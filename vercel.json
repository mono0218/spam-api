{
  "version": 2,
  "public": false,
  "builds": [
    {
      "src": "app/index.py",
      "use": "@now/python"
    }
  ],
  "functions": {
    "api/main.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST,",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
      },
      "src": "/.*",
      "dest": "app/main.py"
    }
  ]
}
